EPTION_EXECUTING(cpu_id) == NU_FALSE)\r\n    {\r\n        \/* Indicate that exception handling is started *\/\r\n        ESAL_GE_EXCEPTION_START(cpu_id);\r\n\r\n\t\t\/* Acquire spinlock for protection of critical section\r\n\t\t   NOTE:  Interrupts are expected to be disabled for duration\r\n\t\t\t\t  of exception handling, so only a spin-lock is\r\n\t\t\t\t  used to protect access to the exception handling list *\/\r\n        Arch_Except_Lock_Acquire(&exception_list_lock);\r\n\r\n        \/* Get the first node in exception list for this vector. *\/\r\n        except_node = exception_list_head[vector_id];\r\n\r\n        \/* Release lock after critical section *\/\r\n        Arch_Lock_Release(&exception_list_lock);\r\n\r\n        \/* Loop through all registered handlers *\/\r\n        while ((except_node != NU_NULL) && (status != NU_EXCEPTION_TERMINATE))\r\n        {\r\n\t\t\t\/* Get pointer to exception structure *\/\r\n\t\t\texcept_ptr = NU_STRUCT_BASE(except_node, node, NU_EXCEPTION);\r\n\r\n\t\t\t\/* Get pointer to data and exception handler *\/\r\n\t\t\tdata = except_ptr->data;\r\n\t\t\tfunc_ptr = except_ptr->func_ptr;\r\n\r\n\t\t\t\/* Call the registered exception handler *\/\r\n\t\t\tstatus = func_ptr(exception_frame, vector_id, data);\r\n\r\n\t\t\tif (status != NU_EXCEPTION_TERMINATE)\r\n\t\t\t{\r\n\t\t\t\t\/* Acquire lock for critical access to exception list. *\/\r\n\t\t\t\tArch_Except_Lock_Acquire(&exception_list_lock);\r\n\r\n\t\t\t\t\/* If the next node in the list is back to the head, set node to NULL. *\/\r\n\t\t\t\tif (list_next_node(exception_list_head[vector_id], &except_node) == NU_TRUE)\r\n\t\t\t\t{\r\n\t\t\t\t    except_node = NU_NULL;\r\n\t\t\t\t}\r\n\r\n\t\t\t\t\/* Release lock after critical section *\/\r\n\t\t\t\tArch_Lock_Release(&exception_list_lock);\r\n\t\t\t}\r\n        }\r\n\r\n        \/* If the exception was not handled *\/\r\n        if (status != NU_EXCEPTION_TERMINATE)\r\n        {\r\n            \/* No exception handler register call system level Exception handler *\/\r\n            (VOID)NU_Handle_Error(NU_UNHANDLED_EXCEPTION);\r\n        }\r\n\r\n        \/* Indicate that exception handling is completed *\/\r\n        ESAL_GE_EXCEPTION_END(cpu_id);\r\n    }\r\n    else\r\n    {\r\n        \/* Nested exceptions are not supported - execute default system Exception handler *\/\r\n        (VOID)NU_Handle_Error(NU_NESTED_EXCEPTION);\r\n    }\r\n}\r\n{code}","issuelinks":[],"customfield_10022":1,"assignee":{"self":"http:\/\/jira.alm.mentorg.com:8080\/rest\/api\/2\/user?username=abachtel","name":"abachtel","key":"abachtel","emailAddress":"Andrew_Bachtel@mentor.com","avatarUrls":{"48x48":"http:\/\/jira.alm.mentorg.com:8080\/secure\/useravatar?avatarId=10262","24x24":"http:\/\/jira.alm.mentorg.com:8080\/secure\/useravatar?size=small&avatarId=10262","16x16":"http:\/\/jira.alm.mentorg.com:8080\/secure\/useravatar?size=xsmall&avatarId=10262","32x32":"http:\/\/jira.alm.mentorg.com:8080\/secure\/useravatar?size=medium&avatarId=10262"},"displayName":"Bachtel, Andrew","active":true,"timeZone":"Europe\/Dublin"},"priority":{"self":"http:\/\/jira.alm.mentorg.com:8080\/rest\/api\/2\/priority\/2","iconUrl":"http:\/\/jira.alm.mentorg.com:8080\/images\/icons\/priorities\/critical.svg","name":"Critical","id":"2"},"aggregatetimespent":null,"status":{"self":"http:\/\/jira.alm.mentorg.com:8080\/rest\/api\/2\/status\/6","description":"The issue is considered finished, the resolution is correct. Issues which are closed can be reopened.","iconUrl":"http:\/\/jira.alm.mentorg.com:8080\/images\/icons\/statuses\/closed.png","name":"Closed","id":"6","statusCategory":{"self":"http:\/\/jira.alm.mentorg.com:8080\/rest\/api\/2\/statuscategory\/3","id":3,"key":"done","colorName":"green","name":"Done"}}}},"NUC4-3577":{"expand":"operations,versionedRepresentations,editmeta,changelog,renderedFields","id":"235244","self":"http:\/\/jira.alm.mentorg.com:8080\/rest\/api\/latest\/issue\/235244","key":"NUC4-3577","fields":{"summary":"Nes